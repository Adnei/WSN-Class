  <plugin>
    org.contikios.cooja.plugins.ScriptRunner
    <plugin_config>
      <script>TIMEOUT(1200000); //10min&#xD;
// TIMEOUT(120000);  // 2min&#xD;
log.log("Time|ID|Message|LISTEN|TRANSMIT\n")&#xD;
while (true) {&#xD;
    // log.log(time + "|" + id + "|" + msg+"\n")&#xD;
    msgArray = msg.split('|')&#xD;
    if(msgArray.length &gt; 1){&#xD;
      // log.log(msg+"\n"+msgArray.length+"\n")&#xD;
      if((msgArray[0]).split(':').length &gt; 1 &amp;&amp; (msgArray[1]).split(':').length &gt; 1){&#xD;
        listen = (msgArray[0]).split(':')[1]&#xD;
        transmit = (msgArray[1]).split(':')[1]&#xD;
        log.log(time + "|" + id + "| |"+listen+"|"+transmit+"\n")&#xD;
      }&#xD;
    }else{&#xD;
      log.log(time + "|" + id + "|" + msg+"|NA|NA\n")&#xD;
    }&#xD;
&#xD;
    YIELD(); /* wait for another mote output */&#xD;
}</script>
      <active>true</active>
    </plugin_config>
    <width>866</width>
    <z>1</z>
    <height>585</height>
    <location_x>261</location_x>
    <location_y>45</location_y>
